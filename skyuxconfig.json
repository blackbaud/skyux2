{
  "name": "skyux2",
  "mode": "easy",
  "importPath": "src",
  "cssPath": "src/scss/sky.scss",
  "compileMode": "aot",
  "omnibar": {
    "experimental": true,
    "nav": {
      "services": [
        {
          "title": "SKY UX 2",
          "searchUrl": "https://stache-search-query.sky.blackbaud.com/search/skyux2",
          "items": [
            {
              "title": "Home",
              "route": "/"
            },
            {
              "title": "Learn",
              "route": "/learn"
            },
            {
              "title": "Components",
              "route": "/components"
            },
            {
              "title": "Design",
              "route": "/design"
            },
            {
              "title": "Contribute",
              "route": "/contribute"
            }
          ]
        }
      ]
    }
  },
  "plugins": [
    "@blackbaud/skyux-builder-plugin-stache"
  ],
  "appSettings": {
    "stache": {
      "searchConfig": {
        "is_internal": false
      }
    }
  },
  "redirects": {
    "learn/get-started/1.-SSL-certification": "learn/get-started/prereqs/ssl-certificate",
    "learn/get-started/2.-initial-setup": "learn/get-started/prereqs/initial-setup",
    "learn/get-started/3.-create-project": "learn/get-started/basics/create-project",
    "learn/get-started/4.-serve-project": "learn/get-started/basics/serve-project",
    "learn/get-started/5.-edit-project": "learn/get-started/basics/edit-project",
    "learn/get-started/6.-add-entry-component": "learn/get-started/advanced/add-entry-components",
    "learn/get-started/7.-run-tests": "learn/get-started/basics/run-tests",
    "learn/get-started/8.-unit-test-modals": "learn/get-started/advanced/unit-test-modals",
    "learn/get-started/9.-include-static-asset": "learn/get-started/advanced/include-static-assets",
    "learn/accessibility/code-review": "learn/accessibility/test",
    "learn/accessibility/keyboard-testing": "learn/accessibility/test",
    "learn/faq": "learn/reference/faq",
    "learn/resources": "learn/reference/resources",
    "learn/get-started/add-entry-components": "learn/get-started/advanced/add-entry-components",
    "learn/get-started/create-project": "learn/get-started/basics/create-project",
    "learn/get-started/serve-project": "learn/get-started/basics/serve-project",
    "learn/get-started/edit-project": "learn/get-started/basics/edit-project",
