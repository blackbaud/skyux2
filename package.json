{
  "name": "blackbaud-skyux2",
  "version": "2.0.0-alpha.5",
  "description": "SKY UX built on Angular 2",
  "author": "Blackbaud, Inc.",
  "homepage": "https://github.com/blackbaud/skyux2",
  "license": "MIT",
  "main": "dist/core.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "ci": "npm run lint && npm run test:ci && npm run build",
    "build": "npm run clean:srctemp && node config/utils/stage-ts.js && cd .srctemp && ../node_modules/.bin/tsc && npm run compile:sass && npm run clean:srctemp",
    "clean": "rimraf -- dist",
    "clean:full": "npm cache clean && rimraf -- node_modules doc coverage dist .srctemp webdriver-screenshotslocal webdriver-screenshotslocal-diffs",
    "clean:releases": "rimraf -- releases",
    "clean:srctemp": "rimraf -- .srctemp",
    "compile:sass": "./node_modules/node-sass/bin/node-sass src/scss/sky.scss dist/css/sky.css",
    "lint": "tslint 'src/**/*.ts'",
    "test": "npm run test:unit && npm run test:visual",
    "test:ci": "npm run test:unit:ci && npm run test:visual:ci",
    "test:unit": "npm run test:unit:base -- config/karma/local.karma.conf.js",
    "test:unit:ci": "npm run test:unit:base -- config/karma/ci.karma.conf.js",
    "test:unit:base": "node --max-old-space-size=4096 node_modules/karma/bin/karma start",
    "test:visual": "npm run test:visual:base -- config/wdio/local.wdio.conf.js",
    "test:visual:ci": "npm run test:visual:base -- config/wdio/ci.wdio.conf.js",
    "test:visual:base": "./node_modules/.bin/wdio",
    "postversion": "git push && git push --tags",
    "prebuild": "npm run clean",
    "pretest": "npm run lint",
    "preversion": "npm test",
    "releases": "npm run clean:releases && ./node_modules/.bin/webpack --progress --config config/webpack/webpack.releases.js",
    "start": "webpack-dev-server --config config/webpack/webpack.dev.js --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
    "start:visual": "node ./config/utils/visual-server.js start",
    "version": "npm run build",
    "watch": "npm run test:unit -- --auto-watch --no-single-run"
  },
  "dependencies": {
    "@angular/common": "2.0.1",
    "@angular/compiler": "2.0.1",
    "@angular/core": "2.0.1",
    "@angular/forms": "2.0.1",
    "@angular/platform-browser": "2.0.1",
    "@angular/platform-browser-dynamic": "2.0.1",
    "@angular/router": "3.0.1",
    "core-js": "2.4.1",
    "dragula": "3.7.2",
    "font-awesome": "4.6.3",
    "ng2-dragula": "1.2.1",
    "ng2-toastr": "0.3.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.23"
  },
  "devDependencies": {
    "@types/core-js": "0.9.34",
    "@types/jasmine": "2.2.34",
    "@types/node": "6.0.41",
    "@types/webpack": "1.12.34",
    "axe-core": "2.0.5",
    "base64-image-loader": "1.0.0",
    "browserstack-local": "1.1.0",
    "codelyzer": "0.0.28",
    "css-loader": "0.25.0",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "font-awesome-webpack": "0.0.4",
    "fs-extra": "0.30.0",
    "glob": "7.1.0",
    "html-webpack-plugin": "2.22.0",
    "http-server": "0.9.0",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine": "2.5.2",
    "json-loader": "0.5.4",
    "karma": "1.3.0",
    "karma-browserstack-launcher": "1.1.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-mocha-reporter": "2.2.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "less": "2.7.1",
    "node-resemble-js": "0.0.5",
    "node-sass": "3.10.0",
    "normalize-scss": "5.0.3",
    "raw-loader": "0.5.1",
    "remap-istanbul": "devCrossNet/remap-istanbul",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "selenium-standalone": "5.6.2",
    "source-map-inline-loader": "blackbaud-bobbyearl/source-map-inline-loader",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.1",
    "ts-helpers": "1.1.1",
    "ts-loader": "0.8.2",
    "tslint": "3.15.1",
    "tslint-loader": "2.1.5",
    "typescript": "2.0.3",
    "url-loader": "0.5.7",
    "wdio-jasmine-framework": "0.2.5",
    "wdio-selenium-standalone-service": "0.0.6",
    "wdio-spec-reporter": "0.0.3",
    "webdrivercss": "v2.0.0beta-rc1",
    "webdriverio": "3.2.6",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.7.0",
    "webpack-dev-server": "1.15.1",
    "webpack-merge": "0.14.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blackbaud/skyux2.git"
  },
  "bugs": {
    "url": "https://github.com/blackbaud/skyux2/issues"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
